{
  "kind": "collectionType",
  "collectionName": "flixes",
  "info": {
    "singularName": "flix",
    "pluralName": "flixes",
    "displayName": "Flix",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "backgroundColor": {
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color"
    },
    "branding": {
      "displayName": "branding",
      "type": "component",
      "repeatable": false,
      "component": "branding.branding"
    },
    "fonts": {
      "type": "customField",
      "options": [
        "Roboto",
        "Open Sans",
        "Lato",
        "Montserrat",
        "Roboto Condensed",
        "Oswald",
        "Poppins",
        "Raleway",
        "Slabo 27px",
        "PT Sans",
        "Noto Sans",
        "Roboto Mono",
        "Roboto Slab",
        "Ubuntu",
        "Merriweather",
        "Lora",
        "Playfair Display",
        "Inter",
        "Nunito",
        "PT Serif"
      ],
      "customField": "plugin::multi-select.multi-select"
    },
    "uri": {
      "type": "string",
      "required": true
    },
    "data": {
      "type": "component",
      "repeatable": false,
      "component": "data-connection.data-connection",
      "required": true
    }
  }
}
